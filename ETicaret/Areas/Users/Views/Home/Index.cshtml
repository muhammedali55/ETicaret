@using Entity.Entities
@model List<Tblkullanici>
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <H1>@ViewBag.karsilama</H1>
    <table>
        <tr>
            <th>Id</th>
            <th>Kullanıcı Adı</th>
            <th>Şifre</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Kullaniciadi</td>
                <td>@item.Sifre</td>
            </tr>
        }
    </table>
</body>
</html>
