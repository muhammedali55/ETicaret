@model  ModelHome
@{
    ViewData["Title"] = "Index";
}

    <main>
        @if (Model.siteOnOff.Where(x => x.id == 1).FirstOrDefault().durum)
        {
            <!-- slider Area Start -->
            <div class="slider-area ">
                <!-- Mobile Menu -->
                <div class="slider-active">
                    @foreach (var item in Model.listeSlider)
                    {

                        <div class="single-slider slider-height" data-background="@item.Backimgurl">
                            <div class="container">
                                <div class="row d-flex align-items-center justify-content-between">
                                    <div class="col-xl-6 col-lg-6 col-md-6 col-sm-6 d-none d-md-block">
                                        <div class="hero__img" data-animation="bounceIn" data-delay=".4s">
                                            <img src="@item.Frontimgurl" alt="">
                                        </div>
                                    </div>
                                    <div class="col-xl-5 col-lg-5 col-md-5 col-sm-8">
                                        <div class="hero__caption">
                                            <span data-animation="fadeInRight" data-delay=".4s">
                                                @item.Discount
                                            </span>
                                            <h1 data-animation="fadeInRight" data-delay=".6s">@Html.Raw(item.Baslik)</h1>
                                            <p data-animation="fadeInRight" data-delay=".8s">@item.Altbaslik</p>
                                            <!-- Hero-btn -->
                                            <div class="hero__btn" data-animation="fadeInRight" data-delay="1s">
                                                <a href="@item.Butonlinkurl" class="btn hero-btn">@item.Butontext</a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    }
                </div>
            </div>
            <!-- slider Area End-->
        }
        @if (Model.siteOnOff.Where(x => x.id == 2).FirstOrDefault().durum)
        {
            <!-- Category Area Start-->
          

            <section class="category-area section-padding30">
                <div class="container-fluid">
                    <!-- Section Tittle -->
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="section-tittle text-center mb-85">
                                <h2>Shop by Category</h2>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        @foreach (var item in Model.kategoriList)
                        {
                            <div class="col-xl-4 col-lg-6">
                                <div class="single-category mb-30">
                                    <div class="category-img">
                                        <img src="assets/img/categori/@item.ResimUrl" alt="">
                                        <div class="category-caption">
                                            <h2>@item.Baslik</h2>
                                            <span class="best"><a href="~/Home/Kategoriler/@item.KategoriId">@item.ButtonText</a></span>
                                            <span class="collection">@item.AltBaslik</span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        }



                    </div>
                </div>
            </section>
            <!-- Category Area End-->
        }
